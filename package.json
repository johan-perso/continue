{
	"private": true,
	"name": "continue",
	"productName": "Continue",
	"description": "Synchroniser les informations d'un appareil Android avec un ordinateur",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"start": "electron .",
		"build-app-x64": "electron-packager . --platform=darwin --arch=x64 --icon=icons/macos.icns --out=release-builds --overwrite",
		"build-app-arm64": "electron-packager . --platform=darwin --arch=arm64 --icon=icons/macos.icns --out=release-builds --overwrite",
		"build": "node build.js"
	},
	"author": "Johan Stickman",
	"license": "MIT",
	"devDependencies": {
		"archiver": "^6.0.1",
		"electron": "^30.0.6",
		"eslint": "^8.48.0"
	},
	"dependencies": {
		"@electron/packager": "^18.3.2",
		"active-window": "^0.1.0",
		"applescript": "^1.0.0",
		"electron-store": "8.1.0",
		"fastify": "^4.26.2"
	}
}
